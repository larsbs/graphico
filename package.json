{
  "name": "yagw",
  "productName": "yagw",
  "version": "1.0.0",
  "main": "app/main/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf app/dist && NODE_ENV=production webpack --config webpack.config.js",
    "build-dev": "rimraf app/dist && NODE_ENV=development WEBPACK_PORT=8081 webpack --config webpack.dev.config.js",
    "build-vendor": "rimraf vendor/custom-graphiql/dist && NODE_ENV=production webpack --config vendor/custom-graphiql/webpack.config.js",
    "webpack-server": "NODE_ENV=development WEBPACK_PORT=8081 webpack-dev-server --config webpack.dev.config.js",
    "dev": "NODE_ENV=development nodemon --exec 'electron .'",
    "start": "NODE_ENV=production electron ."
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "autobind-decorator": "^2.1.0",
    "classnames": "^2.2.5",
    "electron": "^1.8.2",
    "graphiql": "^0.11.11",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-0": "^7.0.0-beta.40",
    "babel-loader": "^8.0.0-beta.0",
    "caniuse-lite": "^1.0.30000810",
    "circular-dependency-plugin": "^4.4.0",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.7",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "ignore-loader": "^0.1.2",
    "ip": "^1.1.5",
    "less": "^3.0.1",
    "less-loader": "^4.0.5",
    "nodemon": "^1.15.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.0",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  }
