{
  "name": "yagw",
  "productName": "Yagw",
  "version": "0.2.0",
  "main": "app/main/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf app/dist && rimraf builds",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "build-dev": "NODE_ENV=development WEBPACK_PORT=8081 webpack --config webpack.dev.config.js",
    "webpack-server": "NODE_ENV=development WEBPACK_PORT=8081 webpack-serve --config webpack.dev.config.js",
    "dev": "NODE_ENV=development WEBPACK_PORT=8081 nodemon --exec 'electron .'",
    "start": "NODE_ENV=production electron .",
    "create-dmg": "electron-installer-dmg builds/Yagw-darwin-x64/Yagw.app yagw --out=builds --icon=./assets/icon.icns",
    "package:mac": "electron-packager --icon=./assets/icon.icns . --out=builds",
    "package": "npm run build && npm run package:mac",
    "create-release": "npm run clean && npm run package && npm run create-dmg"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-rc.2",
    "@babel/register": "^7.0.0-rc.2",
    "aphrodite-jss": "^2.1.0",
    "autobind-decorator": "^2.1.0",
    "classnames": "^2.2.5",
    "electron": "^1.8.8",
    "graphiql": "^0.11.11",
    "graphql": "^0.13.2",
    "modern-normalize": "^0.5.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-transition-group": "^2.2.1",
    "yagw-core": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.2",
    "@babel/plugin-proposal-decorators": "^7.0.0-rc.2",
    "@babel/plugin-proposal-do-expressions": "^7.0.0-rc.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-rc.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0-rc.2",
    "@babel/plugin-proposal-json-strings": "^7.0.0-rc.2",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0-rc.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0-rc.2",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0-rc.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-rc.2",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-rc.2",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0-rc.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.2",
    "@babel/plugin-syntax-import-meta": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-react": "^7.0.0-rc.2",
    "babel-loader": "^8.0.0-beta.6",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^11.2.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "ip": "^1.1.5",
    "koa-connect": "^2.0.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.18.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "react-hot-loader": "^4.3.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  }
}
